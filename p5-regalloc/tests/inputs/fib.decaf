def void test_prime(int a, bool correct)
{
    bool p;
    p = is_prime(a);
    print_bool(p);
    if (p != correct) {
        print_str(" INCORRECT!");
    }
    print_str("\n");
    return;
}


def int main()
{
    test_fact(5, 120);
    test_fact(0, 1);

    test_prime(1, false);
    test_prime(7, true);
    test_prime(8, false);
    test_prime(100, false);
}